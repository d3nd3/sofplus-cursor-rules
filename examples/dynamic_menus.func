function dyn_menus_init()
{
  echo ==maps_init==
  sp_sc_func_exec spf_sc_list_add_func _sp_sv_on_map_begin maps_mapbegin


  //lets have a list of menus, the generate func will iterate them
  zero _dyn_menus_list
  //master
  sp_sc_func_exec spf_sc_list_add_func _dyn_menus_list _dyn_menus_gen_master
  //maps
  sp_sc_func_exec spf_sc_list_add_func _dyn_menus_list _dyn_menus_gen_maps
}

function dyn_menus_mapbegin()
{
  //the menu that receives the dynamic content
  sf_sv_ghoul_register menus/m.rmf
  sf_sv_ghoul_register menus/dynamic/ss_dynamic.rmf
  sf_sv_ghoul_register menus/dynamic/ns_dynamic.rmf
  sf_sv_ghoul_register menus/dynamic/smokey_margin.rmf
  sf_sv_ghoul_register menus/dynamic/content.rmf

  sf_sv_ghoul_register dynamic/dyn_menus_receiver.func
}

//user is in the handshake phase
function .dyn_greet(~slot)
{
  //Process their menu_versions.cfg
  



  set ~data "%6C%BB\%01\"
  sp_sc_cvar_append ~data #~master_rfm
  sp_sv_client_cvar_set $~slot 99 ""
}


